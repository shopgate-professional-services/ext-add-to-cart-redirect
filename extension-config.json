{
  "version": "1.1.0",
  "id": "@shopgate-project/add-to-cart-redirect",
  "components": [
    {
      "id": "IosAddToCartBar",
      "path": "frontend/portals/IosAddToCartBar/index.jsx",
      "target": "product.add-to-cart-bar",
      "type": "portals"
    },
    {
      "id": "AndroidAddToCartButton",
      "path": "frontend/portals/AndroidAddToCartButton/index.jsx",
      "target": "product.ctas.add-to-cart",
      "type": "portals"
    },
    {
      "id": "TabletAddToCartButton",
      "path": "frontend/portals/TabletAddToCartButton/index.jsx",
      "target": "product.tablet.right-column.add-to-cart",
      "type": "portals"
    },
    {
      "id": "FavoritesCTA",
      "path": "frontend/portals/FavoritesCTA/index.jsx",
      "target": "favorites.add-to-cart",
      "type": "portals"
    },
    {
      "type": "translations",
      "path": "frontend/locale/de-DE.json",
      "id": "locale/de-DE"
    },
    {
      "type": "translations",
      "path": "frontend/locale/en-US.json",
      "id": "locale/en-US"
    }
  ],
  "steps": [],
  "configuration": {
    "config": {
      "type": "admin",
      "destination": "frontend",
      "default": {
        "redirects": [
          {
            "ios": true,
            "url": "",
            "android": true,
            "property": "notapple",
            "popupText": "Aufgrund der Apple Nutzungbedingungen ist es uns leider nicht möglich diesen Artikel über die App anzubieten. Wir bitten um Verständnis. Klicken Sie auf \"Weiter\", um zum Artikel im Onlineshop zu gelangen",
            "buttonText": "Auf mobiler Website kaufen",
            "useDeeplink": true,
            "popupHeadline": "Kundeninformation"
          }
        ]
      },
      "params": {
        "label": "Storefront configuration",
        "type": "json"
      }
    }
  }
}
